{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "module": "CommonJS",
    "target": "ES2020",
    "lib": [
      "ES2020"
    ],
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "types": [
      "node",
    ],
    "skipLibCheck": true,
    "paths": {
      "glob": [
        "./node_modules/glob/dist/commonjs"
      ]
    }
  }
}
