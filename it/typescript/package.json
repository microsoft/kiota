{
  "name": "typescript",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "tsc -noEmit && node ./build/esbuild.js --dev",
    "build:meta": "node ./build/esbuild.js --dev --meta",
    "build:meta:prod": "node ./build/esbuild.js --meta",
    "build:prod": "node ./build/esbuild.js",
    "docker:build": "docker build --rm -t typescript:latest .",
    "docker:deploy": "sh ./scripts/deploy.sh",
    "lint": "eslint",
    "prettier": "prettier --write .",
    "prettier:ci": "prettier --check .",
    "start": "node ./build/esbuild.js --dev --watch --run",
    "start:ci": "node ./build/esbuild.js --run",
    "start:prod": "node ./build/esbuild.js --watch --run"
  },
  "prettier": "./.prettierrc.json",
  "devDependencies": {
    "@es-exec/esbuild-plugin-start": "^0.0.5",
    "@stylistic/eslint-plugin-ts": "^4.4.1",
    "@types/node": "^25.0.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.32.1",
    "esbuild": "^0.27.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "minimist": "^1.2.8",
    "prettier": "^3.7.4",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@azure/identity": "^4.13.0",
    "@microsoft/kiota-abstractions": "^1.0.0-preview.99",
    "@microsoft/kiota-authentication-azure": "^1.0.0-preview.99",
    "@microsoft/kiota-http-fetchlibrary": "^1.0.0-preview.99",
    "@microsoft/kiota-serialization-form": "^1.0.0-preview.99",
    "@microsoft/kiota-serialization-json": "^1.0.0-preview.99",
    "@microsoft/kiota-serialization-multipart": "^1.0.0-preview.99",
    "@microsoft/kiota-serialization-text": "^1.0.0-preview.99",
    "express": "^5.2.1",
    "node-fetch": "^2.7.0"
  }
}
