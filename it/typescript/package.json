{
  "name": "typescript",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "node ./build/esbuild.js --dev",
    "build:meta": "node ./build/esbuild.js --dev --meta",
    "build:meta:prod": "node ./build/esbuild.js --meta",
    "build:prod": "node ./build/esbuild.js",
    "docker:build": "docker build --rm -t typescript:latest .",
    "docker:deploy": "sh ./scripts/deploy.sh",
    "lint": "eslint . --ext .ts,.js",
    "prettier": "prettier --write .",
    "prettier:ci": "prettier --check .",
    "start": "node ./build/esbuild.js --dev --watch --run",
    "start:ci": "node ./build/esbuild.js --run",
    "start:prod": "node ./build/esbuild.js --watch --run"
  },
  "prettier": "./.prettierrc.json",
  "devDependencies": {
    "@es-exec/esbuild-plugin-start": "^0.0.5",
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "esbuild": "^0.18.15",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.6.0",
    "minimist": "^1.2.8",
    "prettier": "^3.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@azure/identity": "^3.2.4",
    "@microsoft/kiota-abstractions": "^1.0.0-preview.23",
    "@microsoft/kiota-authentication-azure": "^1.0.0-preview.19",
    "@microsoft/kiota-http-fetchlibrary": "^1.0.0-preview.23",
    "@microsoft/kiota-serialization-form": "^1.0.0-preview.13",
    "@microsoft/kiota-serialization-json": "^1.0.0-preview.22",
    "@microsoft/kiota-serialization-text": "^1.0.0-preview.21",
    "express": "^4.18.2",
    "node-fetch": "^2.6.9"
  }
}
