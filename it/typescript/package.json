{
  "name": "typescript",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "tsc -noEmit && node ./build/esbuild.js --dev",
    "build:meta": "node ./build/esbuild.js --dev --meta",
    "build:meta:prod": "node ./build/esbuild.js --meta",
    "build:prod": "node ./build/esbuild.js",
    "docker:build": "docker build --rm -t typescript:latest .",
    "docker:deploy": "sh ./scripts/deploy.sh",
    "lint": "eslint . --ext .ts,.js",
    "prettier": "prettier --write .",
    "prettier:ci": "prettier --check .",
    "start": "node ./build/esbuild.js --dev --watch --run",
    "start:ci": "node ./build/esbuild.js --run",
    "start:prod": "node ./build/esbuild.js --watch --run"
  },
  "prettier": "./.prettierrc.json",
  "devDependencies": {
    "@es-exec/esbuild-plugin-start": "^0.0.5",
    "@types/node": "^20.12.13",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "esbuild": "^0.21.4",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "minimist": "^1.2.8",
    "prettier": "^3.2.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@azure/identity": "^4.2.0",
    "@microsoft/kiota-abstractions": "^1.0.0-preview.53",
    "@microsoft/kiota-authentication-azure": "^1.0.0-preview.48",
    "@microsoft/kiota-http-fetchlibrary": "^1.0.0-preview.52",
    "@microsoft/kiota-serialization-form": "^1.0.0-preview.42",
    "@microsoft/kiota-serialization-json": "^1.0.0-preview.53",
    "@microsoft/kiota-serialization-multipart": "^1.0.0-preview.31",
    "@microsoft/kiota-serialization-text": "^1.0.0-preview.50",
    "express": "^4.19.2",
    "node-fetch": "^2.7.0"
  }
}