{
   "version": "0.2.0",
   "configurations": [
        {
            "name": "Launch TypeScript",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/kiota/bin/Debug/net6.0/kiota.dll",
            "args": ["generate",
                 "--openapi",
                 "https://raw.githubusercontent.com/microsoftgraph/msgraph-sdk-powershell/dev/openApiDocs/v1.0/Mail.yml",
                 "--language",
                 "typescript",
                 "-o",
                 "${workspaceFolder}/samples/msgraph-mail/typescript/src",
                 "-n",
                 "graphtypescriptv4.utilities" ],
            "cwd": "${workspaceFolder}/src/kiota",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Java",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/kiota/bin/Debug/net6.0/kiota.dll",
            "args": ["generate",
                 "--openapi",
                 "https://raw.githubusercontent.com/microsoftgraph/msgraph-sdk-powershell/dev/openApiDocs/v1.0/Mail.yml",
                 "--language",
                 "java",
                 "-o",
                 "${workspaceFolder}/samples/msgraph-mail/java/utilities/src/main/java/graphjavav4/utilities",
                 "-n",
                 "graphjavav4.utilities" ],
            "cwd": "${workspaceFolder}/src/kiota",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch CSharp",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/kiota/bin/Debug/net6.0/kiota.dll",
            "args": ["generate",
                 "--openapi",
                 "https://raw.githubusercontent.com/microsoftgraph/msgraph-sdk-powershell/dev/openApiDocs/v1.0/Mail.yml",
                 "--language",
                 "csharp",
                 "-o",
                 "${workspaceFolder}/samples/msgraph-mail/dotnet",
                 "-n",
                 "Graphdotnetv4",
                 "--clear-cache" ],
            "cwd": "${workspaceFolder}/src/kiota",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Ruby",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/kiota/bin/Debug/net6.0/kiota.dll",
            "args": ["generate",
                 "--openapi",
                 "https://raw.githubusercontent.com/microsoftgraph/msgraph-sdk-powershell/dev/openApiDocs/v1.0/Mail.yml",
                 "--language",
                 "ruby",
                 "-o",
                 "${workspaceFolder}/samples/msgraph-mail/ruby/graphrubyv4",
                 "-n",
                 "graphrubyv4" ],
            "cwd": "${workspaceFolder}/src/kiota",
            "console": "internalConsole",
            "stopAtEntry": false
        }, 
        {
            "name": "Launch PHP",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/kiota/bin/Debug/net6.0/kiota.dll",
            "args": ["generate",
                 "--openapi",
                  "https://raw.githubusercontent.com/microsoftgraph/msgraph-sdk-powershell/dev/openApiDocs/v1.0/Mail.yml",
                  "--language",
                  "php",
                  "-o",
                  "${workspaceFolder}/samples/msgraph-mail/php/src/",
                  "-n",
                  "Microsoft\\Graph"
            ],
            "cwd": "${workspaceFolder}/src/kiota",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Go",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/kiota/bin/Debug/net6.0/kiota.dll",
            "args": ["generate",
                 "--openapi",
                 "https://raw.githubusercontent.com/microsoftgraph/msgraph-sdk-powershell/dev/openApiDocs/v1.0/Mail.yml",
                 "--language",
                 "go",
                 "-o",
                 "${workspaceFolder}/samples/msgraph-mail/go/utilities",
                 "-n",
                 "github.com/microsoft/kiota-samples/msgraph-mail/go/utilities/" ],
            "cwd": "${workspaceFolder}/src/kiota",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Swift",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/kiota/bin/Debug/net6.0/kiota.dll",
            "args": ["generate",
                 "--openapi",
                 "https://raw.githubusercontent.com/microsoftgraph/msgraph-sdk-powershell/dev/openApiDocs/v1.0/Mail.yml",
                 "--language",
                 "swift",
                 "-o",
                 "${workspaceFolder}/samples/msgraph-mail/Swiftconsoleapp/Sources/Swiftconsoleapp",
                 "-n",
                 "Swiftconsoleapp",
                 "--co" ],
            "cwd": "${workspaceFolder}/src/kiota",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Python",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/kiota/bin/Debug/net6.0/kiota.dll",
            "args": ["generate",
                 "--openapi",
                 "https://raw.githubusercontent.com/microsoftgraph/msgraph-sdk-powershell/dev/openApiDocs/v1.0/Mail.yml",
                 "--language",
                 "python",
                 "-o",
                 "${workspaceFolder}/samples/msgraph-mail/python",
                 "-n",
                 "GraphPythonv1" ],
            "cwd": "${workspaceFolder}/src/kiota",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Search",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/kiota/bin/Debug/net6.0/kiota.dll",
            "args": ["search",
                 "microsoft"
                ],
            "cwd": "${workspaceFolder}/src/kiota",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Show",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/kiota/bin/Debug/net6.0/kiota.dll",
            "args": ["show",
                 "-d",
                 "https://raw.githubusercontent.com/microsoftgraph/msgraph-sdk-powershell/dev/openApiDocs/v1.0/Mail.yml"
                ],
            "cwd": "${workspaceFolder}/src/kiota",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "Launch Download",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/kiota/bin/Debug/net6.0/kiota.dll",
            "args": ["download",
                 "msgraph::microsoft-graph",
                 "-o",
                 "${workspaceFolder}/src/kiota/bin/Debug/net6.0/graphv1.yml"
                ],
            "cwd": "${workspaceFolder}/src/kiota",
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}
